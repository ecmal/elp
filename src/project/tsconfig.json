{
    "name":"@vendor/project",
    "version":"0.0.1",
    "resources": {
        "png"   : "image/png",
        "html"  : "text/html"
    },
    "compilerOptions": {
        "module": "system",
        "target": "es5",
        "declaration": true,
        "sourceMap": true,
        "outDir": "../../node_modules",
        "noEmitHelpers": true,
        "importHelpers": true,
        "stripInternal": true,
        "baseUrl": "../../node_modules",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "jsx": "react",
        "sourceRoot": "."
    }
}