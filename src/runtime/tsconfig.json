{
    "name"                  : "@ecmal/runtime",
    "version"               : "0.0.3",
    "description"           : "Ecmal Runtime",
    "license"               : "UNLICENSED",
    "author"                : "Mamble Inc. <ecmal@mamble.co>",
    "repository"            : "https://github.com/ecmal/elp.git",
    "contributors"          : ["Sergey Mamyan <sergey@mamble.co>"],
    "homepage"              : "https://github.com/ecmal/elp",
    "bugs"                  : "https://github.com/ecmal/elp/issues",
    "keywords"              : ["typescript","ecmal","compiler"],
    "types"                 : "index.d.ts",
    "bin"                   : {
        "elr"               : "./bin/cli.js"
    },
    "compilerOptions"       : {
        "module"            : "system",
        "target"            : "es5",
        "declaration"       : true,
        "sourceMap"         : true,
        "sourceRoot"        : ".",
        "baseUrl"           : "../../node_modules",
        "outDir"            : "../../node_modules",
        "noEmitHelpers"     : true,
        "importHelpers"     : true,
        "stripInternal"     : true
    }
}